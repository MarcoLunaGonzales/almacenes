	<queryString><![CDATA[SELECT
     ORDENES_COMPRA."COD_ORDEN_COMPRA" AS ORDENES_COMPRA_COD_ORDEN_COMPRA,
     ORDENES_COMPRA."NRO_ORDEN_COMPRA" AS ORDENES_COMPRA_NRO_ORDEN_COMPRA,
     ORDENES_COMPRA."FECHA_EMISION" AS ORDENES_COMPRA_FECHA_EMISION,
     ORDENES_COMPRA."DIVISION" AS ORDENES_COMPRA_DIVISION,
     ORDENES_COMPRA."FECHA_ENTREGA" AS ORDENES_COMPRA_FECHA_ENTREGA,
     ORDENES_COMPRA."DESC_FECHA_ENTREGA" AS ORDENES_COMPRA_DESC_FECHA_ENTREGA,
     ORDENES_COMPRA."OBS_ORDEN_COMPRA" AS ORDENES_COMPRA_OBS_ORDEN_COMPRA,
     ORDENES_COMPRA."EMITIR_CHEQUEANOMBREDE" AS ORDENES_COMPRA_EMITIR_CHEQUEANOMBREDE,
     ORDENES_COMPRA."DIAS_TERMINOSPAGO" AS ORDENES_COMPRA_DIAS_TERMINOSPAGO,
     ORDENES_COMPRA."OBS_ORDEN_COMPRA_APROBACION" AS ORDENES_COMPRA_OBS_ORDEN_COMPRA_APROBACION,
     ORDENES_COMPRA_DETALLE."CANTIDAD_NETA" AS ORDENES_COMPRA_DETALLE_CANTIDAD_NETA,
     ORDENES_COMPRA_DETALLE."PRECIO_UNITARIO" AS ORDENES_COMPRA_DETALLE_PRECIO_UNITARIO,
     ORDENES_COMPRA_DETALLE."PRECIO_TOTAL" AS ORDENES_COMPRA_DETALLE_PRECIO_TOTAL,
     ORDENES_COMPRA_DETALLE."DESCRIPCION" AS ORDENES_COMPRA_DETALLE_DESCRIPCION,
     PROVEEDORES."NOMBRE_PROVEEDOR" AS PROVEEDORES_NOMBRE_PROVEEDOR,
     PROVEEDORES."CIUDAD_PROVEEDOR" AS PROVEEDORES_CIUDAD_PROVEEDOR,
     PROVEEDORES."DIRECCION_PROVEEDOR" AS PROVEEDORES_DIRECCION_PROVEEDOR,
     PROVEEDORES."EMAIL_PROVEEDOR" AS PROVEEDORES_EMAIL_PROVEEDOR,
     PROVEEDORES."TELEFONO_PROVEEDOR" AS PROVEEDORES_TELEFONO_PROVEEDOR,
     PROVEEDORES."FAX_PROVEEDOR" AS PROVEEDORES_FAX_PROVEEDOR,
     PROVEEDORES."PAGINAWEB_PROVEEDOR" AS PROVEEDORES_PAGINAWEB_PROVEEDOR,
     PROVEEDORES."OBS_PROVEEDOR" AS PROVEEDORES_OBS_PROVEEDOR,
     MEDIOS_PAGO."NOMBRE_MEDIOPAGO" AS MEDIOS_PAGO_NOMBRE_MEDIOPAGO,
     TIPOS_PAGO."NOMBRE_TIPO_PAGO" AS TIPOS_PAGO_NOMBRE_TIPO_PAGO,
     TIPOS_TRANSPORTE."NOMBRE_TIPO_TRANSPORTE" AS TIPOS_TRANSPORTE_NOMBRE_TIPO_TRANSPORTE,
     TIPOS_DOCUMENTOS_COMPRAS."NOMBRE_TIPO_DOCUMENTO" AS TIPOS_DOCUMENTOS_COMPRAS_NOMBRE_TIPO_DOCUMENTO,
     ALMACENES."NOMBRE_ALMACEN" AS ALMACENES_NOMBRE_ALMACEN,
     UNIDADES_MEDIDA."NOMBRE_UNIDAD_MEDIDA" AS UNIDADES_MEDIDA_NOMBRE_UNIDAD_MEDIDA,
     UNIDADES_MEDIDA."ABREVIATURA" AS UNIDADES_MEDIDA_ABREVIATURA,
     CONTACTOS_PROVEEDOR."AP_PATERNO_CONTACTO" AS CONTACTOS_PROVEEDOR_AP_PATERNO_CONTACTO,
     CONTACTOS_PROVEEDOR."AP_MATERNO_CONTACTO" AS CONTACTOS_PROVEEDOR_AP_MATERNO_CONTACTO,
     CONTACTOS_PROVEEDOR."NOMBRE_CONTACTO" AS CONTACTOS_PROVEEDOR_NOMBRE_CONTACTO,
     GRADOS_ACADEMICOS."ABREVIATURA_GRADO_ACADEMICO" AS GRADOS_ACADEMICOS_ABREVIATURA_GRADO_ACADEMICO,
     PERSONAL."AP_MATERNO_PERSONAL" AS PERSONAL_AP_MATERNO_PERSONAL,
     PERSONAL."AP_PATERNO_PERSONAL" AS PERSONAL_AP_PATERNO_PERSONAL,
     PERSONAL."NOMBRE_PILA" AS PERSONAL_NOMBRE_PILA,
     REPRESENTANTES."NOMBRE_REPRESENTANTE" AS REPRESENTANTES_NOMBRE_REPRESENTANTE,
     REPRESENTANTES."CIUDAD_REPRESENTANTE" AS REPRESENTANTES_CIUDAD_REPRESENTANTE,
     REPRESENTANTES."DIRECCION_REPRESENTANTE" AS REPRESENTANTES_DIRECCION_REPRESENTANTE,
     REPRESENTANTES."TELEFONO_REPRESENTANTE" AS REPRESENTANTES_TELEFONO_REPRESENTANTE,
     REPRESENTANTES."EMAIL_REPRESENTANTE" AS REPRESENTANTES_EMAIL_REPRESENTANTE,
     REPRESENTANTES."FAX_REPRESENTANTE" AS REPRESENTANTES_FAX_REPRESENTANTE,
     PAISES."NOMBRE_PAIS" AS PAISES_NOMBRE_PAIS,
     PAISES_A."NOMBRE_PAIS" AS PAISES_A_NOMBRE_PAIS,
     isnull(CONTACTOS_REPRESENTANTES."AP_PATERNO_CONTACTO",'') AS CONTACTOS_REPRESENTANTES_AP_PATERNO_CONTACTO,
     isnull(CONTACTOS_REPRESENTANTES."AP_MATERNO_CONTACTO",'') AS CONTACTOS_REPRESENTANTES_AP_MATERNO_CONTACTO,
     isnull(CONTACTOS_REPRESENTANTES."NOMBRE_CONTACTO",'') AS CONTACTOS_REPRESENTANTES_NOMBRE_CONTACTO,
     CONTACTOS_REPRESENTANTES."CARGO_CONTACTO" AS CONTACTOS_REPRESENTANTES_CARGO_CONTACTO,
     MATERIALES."NOMBRE_COMERCIAL_MATERIAL" AS MATERIALES_NOMBRE_COMERCIAL_MATERIAL,
     MONEDAS."COD_MONEDA" AS MONEDAS_COD_MONEDA,
     TIPOS_CENTRO_DE_COSTO."NOMBRE_TIPO_CENTRO_COSTOS" AS TIPOS_CENTRO_DE_COSTO_NOMBRE_TIPO_CENTRO_COSTOS,
     PLAN_DE_CUENTAS."NOMBRE_CUENTA" AS PLAN_DE_CUENTAS_NOMBRE_CUENTA,
     CONDICIONES_PRECIO."NOMBRE_CONDICION_PRECIO" AS CONDICIONES_PRECIO_NOMBRE_CONDICION_PRECIO,
     ORDENES_COMPRA."FECHA_DESPACHO" AS ORDENES_COMPRA_FECHA_DESPACHO,
     CONDICIONESPRECIO_LUGARESENTREGA."NOMBRE_LUGARENTREGA" AS CONDICIONESPRECIO_LUGARESENTREGA_NOMBRE_LUGARENTREGA
FROM
     "dbo"."ORDENES_COMPRA_DETALLE" ORDENES_COMPRA_DETALLE INNER JOIN "dbo"."ORDENES_COMPRA" ORDENES_COMPRA ON ORDENES_COMPRA_DETALLE."COD_ORDEN_COMPRA" = ORDENES_COMPRA."COD_ORDEN_COMPRA"
     LEFT OUTER JOIN "dbo"."DISTRIBUCION_GASTOS_COMPRAS" DISTRIBUCION_GASTOS_COMPRAS ON ORDENES_COMPRA."COD_ORDEN_COMPRA" = DISTRIBUCION_GASTOS_COMPRAS."COD_ORDEN_COMPRA"
     INNER JOIN "dbo"."PROVEEDORES" PROVEEDORES ON ORDENES_COMPRA."COD_PROVEEDOR" = PROVEEDORES."COD_PROVEEDOR"
     INNER JOIN "dbo"."ALMACENES" ALMACENES ON ORDENES_COMPRA."COD_ALMACEN_ENTREGA" = ALMACENES."COD_ALMACEN"
     INNER JOIN "dbo"."TIPOS_DOCUMENTOS_COMPRAS" TIPOS_DOCUMENTOS_COMPRAS ON ORDENES_COMPRA."COD_TIPO_DOCUMENTO_TERMINOSPAGO" = TIPOS_DOCUMENTOS_COMPRAS."COD_TIPO_DOCUMENTO"
     INNER JOIN "dbo"."TIPOS_TRANSPORTE" TIPOS_TRANSPORTE ON ORDENES_COMPRA."COD_TIPO_TRANSPORTE" = TIPOS_TRANSPORTE."COD_TIPO_TRANSPORTE"
     INNER JOIN "dbo"."TIPOS_PAGO" TIPOS_PAGO ON ORDENES_COMPRA."COD_TIPO_PAGO" = TIPOS_PAGO."COD_TIPO_PAGO"
     INNER JOIN "dbo"."MEDIOS_PAGO" MEDIOS_PAGO ON ORDENES_COMPRA."COD_MEDIOPAGO" = MEDIOS_PAGO."COD_MEDIOPAGO"
     INNER JOIN "dbo"."PERSONAL" PERSONAL ON ORDENES_COMPRA."COD_RESPONSABLE_COMPRAS" = PERSONAL."COD_PERSONAL"
     LEFT OUTER JOIN "dbo"."REPRESENTANTES" REPRESENTANTES ON ORDENES_COMPRA."COD_REPRESENTANTE" = REPRESENTANTES."COD_REPRESENTANTE"
     INNER JOIN "dbo"."MONEDAS" MONEDAS ON ORDENES_COMPRA."COD_MONEDA" = MONEDAS."COD_MONEDA"
     INNER JOIN "dbo"."CONDICIONES_PRECIO" CONDICIONES_PRECIO ON ORDENES_COMPRA."COD_CONDICION_PRECIO" = CONDICIONES_PRECIO."COD_CONDICION_PRECIO"
     RIGHT OUTER JOIN "dbo"."CONDICIONESPRECIO_LUGARESENTREGA" CONDICIONESPRECIO_LUGARESENTREGA ON ORDENES_COMPRA."COD_CONDICION_PRECIO" = CONDICIONESPRECIO_LUGARESENTREGA."COD_CONDICION_PRECIO"
     LEFT OUTER JOIN "dbo"."CONTACTOS_REPRESENTANTES" CONTACTOS_REPRESENTANTES ON REPRESENTANTES."COD_REPRESENTANTE" = CONTACTOS_REPRESENTANTES."COD_REPRESENTANTE"
     LEFT OUTER JOIN "dbo"."PAISES" PAISES_A ON REPRESENTANTES."COD_PAIS" = PAISES_A."COD_PAIS"
     INNER JOIN "dbo"."GRADOS_ACADEMICOS" GRADOS_ACADEMICOS ON PERSONAL."COD_GRADO_ACADEMICO" = GRADOS_ACADEMICOS."COD_GRADO_ACADEMICO"
     LEFT OUTER JOIN "dbo"."CONTACTOS_PROVEEDOR" CONTACTOS_PROVEEDOR ON PROVEEDORES."COD_PROVEEDOR" = CONTACTOS_PROVEEDOR."COD_PROVEEDOR"
     LEFT OUTER JOIN "dbo"."PAISES" PAISES ON PROVEEDORES."COD_PAIS" = PAISES."COD_PAIS"
     INNER JOIN "dbo"."TIPOS_CENTRO_DE_COSTO" TIPOS_CENTRO_DE_COSTO ON DISTRIBUCION_GASTOS_COMPRAS."COD_DIVISION" = TIPOS_CENTRO_DE_COSTO."COD_TIPO_CENTRO_COSTOS"
     INNER JOIN "dbo"."PLAN_DE_CUENTAS" PLAN_DE_CUENTAS ON DISTRIBUCION_GASTOS_COMPRAS."COD_CUENTA" = PLAN_DE_CUENTAS."COD_PLAN_CUENTA"
     INNER JOIN "dbo"."UNIDADES_MEDIDA" UNIDADES_MEDIDA ON ORDENES_COMPRA_DETALLE."COD_UNIDAD_MEDIDA" = UNIDADES_MEDIDA."COD_UNIDAD_MEDIDA"
     INNER JOIN "dbo"."MATERIALES" MATERIALES ON ORDENES_COMPRA_DETALLE."COD_MATERIAL" = MATERIALES."COD_MATERIAL"

WHERE
     ORDENES_COMPRA."COD_ORDEN_COMPRA" = $P{codOrdenCompra}
     and CONTACTOS_PROVEEDOR."COD_ESTADO_CONTACTO" = 1
     and CONTACTOS_PROVEEDOR."CONTACTO_PROVEEDOR_ACTIVO" = 1]]></queryString>

